{
    "name": "react-payphone-js",
    "version": "0.2.2",
    "description": "A Button react.js for payphone checkout",
    "type": "module",
    "author": {
        "name": "Royaltics Solutions",
        "email": "dev@royaltics.com"
    },
    "keywords": [
        "reactjs",
        "payphone",
        "checkout",
        "payment",
        "button",
        "button paypal javascript payphone"
    ],
    "license": "MIT",
    "repository": "https://github.com/royaltics-solutions/react-payphone-js.git",
    "main": "dist/index.js",
    "module": "dist/index.umd.cjs",
    "source": "src/index.tsx",
    "types": "./dist/main.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.umd.cjs"
        }
    },
    "sideEffects": false,
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "dev": "vite",
        "prebuild": "if exist dist rmdir /s /q dist",
        "build": "tsc -b && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "prepublish": "npm run build"
    },
    "peerDependencies": {
        "react": "^19.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.17.0",
        "@microsoft/api-extractor": "^7.55.2",
        "@types/jest": "^29.5.14",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@vitejs/plugin-react": "^4.3.4",
        "eslint": "^9.17.0",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.16",
        "globals": "^15.14.0",
        "typescript": "~5.6.2",
        "typescript-eslint": "^8.18.2",
        "vite": "^6.0.5",
        "vite-plugin-dts": "^4.4.0"
    },
    "files": [
        "dist"
    ]
}
